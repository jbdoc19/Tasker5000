modules = ["python-3.11"]
[agent]
expertMode = true

[[ports]]
localPort = 5000
externalPort = 80

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Tasker5000 Server"

[[workflows.workflow]]
name = "Tasker5000 Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd tasker5000 && python -m uvicorn main:app --host 0.0.0.0 --port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"
